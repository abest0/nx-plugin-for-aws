{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../dist/e2e",
    "declaration": true,
    "types": [
      "node"
    ],
    "tsBuildInfoFile": "../dist/e2e/tsconfig.lib.tsbuildinfo"
  },
  "exclude": [
    "vite.config.ts",
    "vite.config.mts",
    "vitest.config.ts",
    "vitest.config.mts",
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "src/**/*.test.tsx",
    "src/**/*.spec.tsx",
    "src/**/*.test.js",
    "src/**/*.spec.js",
    "src/**/*.test.jsx",
    "src/**/*.spec.jsx"
  ]
}
